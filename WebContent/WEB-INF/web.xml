<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>eXceedVote</display-name>
  <welcome-file-list>
    <welcome-file>LoginPage.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<servlet-class>com.ckp.controller.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/login-servlet</url-pattern>
  </servlet-mapping>
 
 <servlet>
  	<servlet-name>VoteServlet</servlet-name>
  	<servlet-class>com.ckp.controller.VoteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>VoteServlet</servlet-name>
  	<url-pattern>/vote-servlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>AddProjectServlet</servlet-name>
  	<servlet-class>com.ckp.controller.AddProjectServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>AddProjectServlet</servlet-name>
  	<url-pattern>/addproject-servlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>EditProjectServlet</servlet-name>
  	<servlet-class>com.ckp.controller.EditProjectServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>EditProjectServlet</servlet-name>
  	<url-pattern>/editproject-servlet</url-pattern>
  </servlet-mapping>
</web-app>