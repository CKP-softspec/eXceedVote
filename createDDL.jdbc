CREATE TABLE USER (ID INTEGER AUTO_INCREMENT NOT NULL, LASTNAME VARCHAR(255), NAME VARCHAR(255), PASSWORD VARCHAR(255), ROLE VARCHAR(255), USERNAME VARCHAR(255), PRIMARY KEY (ID))
CREATE TABLE PROJECT (ID INTEGER AUTO_INCREMENT NOT NULL, IMGURL1 VARCHAR(255), IMGURL2 VARCHAR(255), IMGURL3 VARCHAR(255), PROJECTDETAIL VARCHAR(255), PROJECTNAME VARCHAR(255), SHORTPROJECTDETAIL VARCHAR(255), PRIMARY KEY (ID))
CREATE TABLE QUESTION (ID INTEGER AUTO_INCREMENT NOT NULL, TITLE VARCHAR(255), PRIMARY KEY (ID))
CREATE TABLE TEAM (ID INTEGER AUTO_INCREMENT NOT NULL, NAME VARCHAR(255), PRIMARY KEY (ID))
CREATE TABLE TEAMMEMBER (ID INTEGER AUTO_INCREMENT NOT NULL, TEAMID INTEGER, USERID INTEGER, PRIMARY KEY (ID))
CREATE TABLE VOTE (ID INTEGER AUTO_INCREMENT NOT NULL, PROJECTID INTEGER, QUESTIONID INTEGER, TIMESTAMPFIELD DATETIME, USERID INTEGER, PRIMARY KEY (ID))
CREATE TABLE LOGIN_LOG (ID INTEGER AUTO_INCREMENT NOT NULL, IP VARCHAR(255), TIMESTAMPFIELD DATETIME, PRIMARY KEY (ID))
